filters:
  and:
    - file.hasTag("张宇1000题")
views:
  - type: table
    name: 张宇1000题
    filters:
      or:
        - file.name == "张宇1000题基础篇"
        - file.name == "张宇1000题强化篇"
    order:
      - file.name
      - 创建时间
      - 考试科目
      - 课程
      - 阶段
      - 老师
      - 开始日期
      - 结束日期
      - file.links
      - file.backlinks
    sort:
      - property: file.ctime
        direction: ASC
