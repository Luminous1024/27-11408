filters:
  and:
    - file.hasTag("线性代数")
views:
  - type: table
    name: 表格
    filters:
      and:
        - 课程 == "考研数学基础30讲 —— 线性代数"
        - file.name != "考研数学基础30讲 —— 线性代数"
    order:
      - file.name
      - 创建时间
      - 考试科目
      - 课程
      - 阶段
      - 老师
      - 开始日期
      - 结束日期
      - file.links
      - file.backlinks
    sort:
      - property: file.ctime
        direction: ASC
